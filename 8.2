from PIL import Image
cards = {
    "Birthday": "1.jpg",
    "New Year": "2.jpg",
    "Valentine's Day": "3.jpg"
}
def show_card(holiday):
    if holiday in cards:
        file_name = cards[holiday]
        card = Image.open(file_name)
        card.show()
    else:
        print("Card for this holiday not found.")
holiday = input("For which holiday do you need a card? ")
show_card(holiday)
